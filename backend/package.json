{
  "name": "vuttr-backend",
  "version": "1.0.0",
  "description": "Very Useful Tools to Remember API",
  "main": "index.js",
  "author": "gusflopes <gusflopes86@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon src/server.js",
    "dev:debug": "nodemon --inspect src/server.js",
    "build": "sucrase ./src -d ./dist --transforms imports",
    "start": "node dist/server.js",
    "test:setup": "docker start postgres && NODE_ENV=test sequelize db:create test && docker stop postgres",
    "pretest": "docker start postgres && NODE_ENV=test sequelize db:migrate",
    "test": "NODE_ENV=test jest --runInBand --watchAll",
    "posttest": "NODE_ENV=test sequelize db:migrate:undo:all && docker-compose stop"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^7.14.0",
    "pg-hstore": "^2.3.3",
    "pm2": "^4.2.0",
    "sequelize": "^5.21.2",
    "youch": "^2.0.10",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "@sucrase/jest-plugin": "^2.0.0",
    "@types/jest": "^24.0.23",
    "eslint": "^6.7.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "factory-girl": "^5.0.4",
    "faker": "^4.1.0",
    "jest": "^24.9.0",
    "nodemon": "^2.0.1",
    "prettier": "^1.19.1",
    "sequelize-cli": "^5.5.1",
    "sucrase": "^3.10.1",
    "supertest": "^4.0.2"
  }
}
